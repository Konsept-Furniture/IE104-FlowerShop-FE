{
   "compilerOptions": {
      "target": "ES5",
      "strict": true,
      "module": "ESNext",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
         "@/*": ["src/*"],
         "@@/*": ["./*"]
      }
   },
   "include": ["src/**/*"]
}
